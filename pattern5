/******************************************************************************

if n=5
a
a b 
a b c 
a b c d 
a b c d e
**************************************************************/
#include <stdio.h>

int
main ()
{
  //printf("Hello World");
  int n, i;
  char c;
  scanf ("%d", &n);
  for (i = 1; i <= n; i++, printf ("\n"))
    {
      c = 'a';
      for (int j = 1; j <= i; j++)
	{
	  printf ("%c", c++);
	  //c++;
	}
    }
  return 0;
}

