/******************************************************************************
if n=4
1111
222
33
4
*******************************************************************************/
#include <stdio.h>

int
main ()
{
  //printf("Hello World");
  int n;
  scanf ("%d", &n);
  for (int i = 1; i <= n; i++, printf ("\n"))
    {
      for (int j = i; j <= n; j++)
	printf ("%d", i);
    }
  return 0;
}

