/******************************************************************************

input 5

output

1
11
111
1111
11111

*******************************************************************************/
#include <stdio.h>

int main()
{
    //printf("Hello World");
    int n;
    scanf("%d",&n);
    for(int i=1;i<=n;i++,printf("\n")){
        for(int j=1;j<=i;j++){
            printf("1");
        }
    }
    return 0;
}
