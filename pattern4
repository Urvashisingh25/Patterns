#include <stdio.h>

int
main ()
{
  //printf("Hello World");
  int n;
  scanf ("%d", &n);
  for (int i = 1; i <= n; i++, printf ("\n"))
    {
      for (int j = 1; j <= 2 * i; j++)
	{
	  printf ("%d", i);
	}
    }

  return 0;
}

