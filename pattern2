/******************************************************************************

if num=3
print
11
1111
111111

*******************************************************************************/
#include <stdio.h>

int
main ()
{
  int n;
  scanf ("%d", &n);
  for (int i = 1; i <= n; i++, printf ("\n"))
    {
      for (int j = 1; j <= 2 * i; j++)
	{
	  printf ("1");
	}
    }


  return 0;
}

